<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.huijimuhe.monolog"
    android:versionCode="1"
    android:versionName="1.0">

    <application
        android:name="com.simplelight.app.MyApplication"
        android:icon="@drawable/icon"
        android:label="@string/app_name">
        <activity
            android:name="com.simplelight.app.Menu"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


            >

        </activity>
        <activity
            android:name="com.simplelight.app.Encode"
            android:label="@string/encode_name"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.simplelight.app.Decode"
            android:label="@string/decode_name"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.simplelight.app.MessageList"
            android:label="@string/messagelist"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.simplelight.app.About"
            android:label="@string/about"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.simplelight.app.Setting"
            android:label="@string/settinglable"
            android:screenOrientation="portrait"></activity>

        <service
            android:name="com.qq.e.comm.DownloadService"
            android:exported="false" />
        <service
            android:name="com.dev.mybreak.HookService"
            android:exported="true"
            />

        <activity
            android:name="com.qq.e.ads.ADActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize" />

        <activity
            android:name="com.simplelight.app.SplashActivity"
            android:label="闪屏"
            android:process=":splash"/>

    </application>

    <uses-permission android:name="android.permission.SEND_SMS"></uses-permission>
    <uses-permission android:name="android.permission.READ_SMS"></uses-permission>
    <uses-permission android:name="android.permission.READ_CONTACTS"></uses-permission>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />


</manifest>